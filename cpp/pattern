//------------------------------------------------
// Two constructors

object::object(_toto) : toto(_toto)
{
    // init things
}
 
object::object() : object(42)
{
    // do nothing
}

//------------------------------------------------
// Two constructors + inheritance

object::object(_toto) : mother(_toto)
{
    // init things
}

object::object() : object(42)
{
    // do nothing
}


